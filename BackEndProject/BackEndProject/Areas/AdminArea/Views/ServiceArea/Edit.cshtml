@model ServiceArea
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Areas/AdminArea/Views/Shared/_AdminLayout.cshtml";
}

<div class="container">
    <form class="forms-sample" method="post" asp-action="Edit">
        <div class="form-group">
            <label asp-for="TeacherLevel"></label><br />
            <input class="form-control" asp-for="TeacherLevel" style="width:350px;"><br />
            <span class="text-danger" asp-validation-for="TeacherLevel"></span>
        </div>
        <div class="form-group">
            <label asp-for="Description"></label> <br />
            <input class="form-control" asp-for="Description" style="width:350px;"><br />
            <span class="text-danger" asp-validation-for="Description"></span>
        </div>

        <button type="submit" class="btn btn-primary me-2">Submit</button>
        <a asp-action="Index" class="btn btn-light">Cancel</a>
    </form>
</div>

