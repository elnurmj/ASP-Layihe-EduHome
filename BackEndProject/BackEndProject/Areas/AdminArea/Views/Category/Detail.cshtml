@model Category
@{
    ViewData["Title"] = "Detail";
    Layout = "~/Areas/AdminArea/Views/Shared/_AdminLayout.cshtml";
}

<div class="container">
    <div class="form-group">
        <label asp-for="CategorySection"></label><br />
        <input class="form-control" asp-for="CategorySection" style="width:350px;"><br />
        <span class="text-danger" asp-validation-for="CategorySection"></span>
    </div>
    <div style="width:300px;">

        <a asp-action="Index" class="btn btn-light">Cancel</a>
    </div>
</div>

